{
  "extends": "@spotify/tsconfig",
  "include": ["src"],
  "exclude": ["node_modules", "**/__tests__/*"],
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "isolatedModules": false,
    "noEmit": false,
    "noImplicitThis": false,
    "sourceMap": false
  }
}
